{
    "name": "FloatNote",
    "productName": "FloatNote",
    "version": "0.5.0",
    "description": "Lightweight floatnote sticky notes",
    "type": "module",
    "main": "src/index.js",
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "echo \"No linting configured\"",
        "test": "vitest run",
        "build": "vite build",
        "package-light": "npm run build ; npx electron-packager . --platform=win32 --arch=x64 --out=dist --asar --prune --ignore=\"\\.git|\\.vscode|out|node_modules|\\.log$|test|tests|\\.DS_Store|Thumbs.db|\\.prettierrc|\\.prettierignore|vitest\\.config\\.js|README\\.md|prd\\.txt|entitlements\\.plist\""
    },
    "keywords": [
        "notes",
        "sticky notes",
        "to-do",
        "electron"
    ],
    "author": "winshaurya",
    "dependencies": {
        "auto-launch": "^5.0.6",
        "better-sqlite3": "^11.7.0"
    },
    "devDependencies": {
        "@electron-forge/cli": "^7.10.2",
        "@electron-forge/maker-squirrel": "^7.10.2",
        "@electron-forge/maker-zip": "^7.10.2",
        "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
        "@electron-forge/plugin-fuses": "^7.10.2",
        "@electron-forge/plugin-vite": "^7.10.2",
        "@electron/fuses": "^1.8.0",
        "electron": "^30.0.5",
        "electron-builder": "^26.0.12",
        "electron-packager": "^17.1.2",
        "vite": "^7.1.10",
        "vitest": "^1.6.1"
    }
}
